{
    "compilerOptions": {
        "target": "ES2020",
        "module": "CommonJS",
        "outDir": "lib",
        "strict": true,
        "declaration": true,
        "declarationDir": "typings",
        "sourceMap": true,
        "esModuleInterop": true
    },
    "include": [
        "src"
    ],
    "exclude": [
        "node_modules",
        "lib"
    ],
    "typedocOptions": {
        "entryPoints": [
            "src/",
        ],
        "exclude": [],
        "theme": [
            "default"
        ],
        "out": "docs",
        "readme": "README.md",
        "name": "Velox.db"
    }
}